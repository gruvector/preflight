{
  "name": "@upleveled/preflight",
  "version": "5.0.2",
  "repository": "upleveled/preflight",
  "license": "MIT",
  "author": "UpLeveled (https://github.com/upleveled)",
  "contributors": [
    "José Fernando Höwer Barbosa <josehowerupn@gmail.com>",
    "Karl Horky <karl.horky@gmail.com>"
  ],
  "type": "module",
  "main": "dist/preflight.esm.js",
  "module": "dist/preflight.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "preflight": "bin/preflight.js"
  },
  "files": [
    "bin/preflight.js",
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build --format esm --target node --tsconfig tsconfig.src.json",
    "docker-build": "docker build --tag preflight .",
    "docker-build-run": "pnpm docker-build-ts && pnpm docker-build && pnpm docker-run",
    "docker-build-ts": "tsc --project docker/tsconfig.build.json",
    "docker-run": "docker run preflight",
    "postinstall": "patch-package",
    "lint": "eslint . --max-warnings 0",
    "prepare": "pnpm build",
    "size": "size-limit",
    "start": "tsdx watch --format esm --target node",
    "test": "tsdx test",
    "test-local": "rm -rf ./__tests__/fixtures/__temp && pnpm test"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!execa)/"
    ]
  },
  "dependencies": {
    "@types/eslint": "8.56.2",
    "algoliasearch": "4.22.1",
    "chalk": "5.3.0",
    "cheerio": "1.0.0-rc.12",
    "depcheck": "1.4.7",
    "domhandler": "5.0.3",
    "execa": "8.0.1",
    "listr2": "8.0.1",
    "node-fetch": "3.3.2",
    "p-reduce": "3.0.0",
    "patch-package": "8.0.0",
    "prettier": "3.2.4",
    "readdirp": "3.6.0",
    "semver": "7.5.4",
    "top-user-agents": "2.1.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.23.3",
    "@jest/globals": "29.7.0",
    "@size-limit/file": "11.0.2",
    "@types/babel__core": "7.20.5",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.6",
    "@types/p-map": "2.0.0",
    "@types/semver": "7.5.6",
    "babel-jest": "29.7.0",
    "eslint": "8.56.0",
    "eslint-config-upleveled": "7.5.0",
    "p-map": "7.0.1",
    "postinstall-postinstall": "2.1.0",
    "size-limit": "11.0.2",
    "stylelint": "16.2.0",
    "tsdx": "0.14.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "6.15.0",
      "@typescript-eslint/parser": "6.15.0",
      "@typescript-eslint/scope-manager": "6.15.0",
      "@typescript-eslint/utils": "6.15.0",
      "eslint": "8.53.0",
      "eslint-plugin-jest": "27.6.0",
      "eslint-plugin-react-hooks": "4.6.0",
      "node-notifier": "10.0.1",
      "normalize-package-data@2.5.0>semver": "5.7.2",
      "jest": "29.5.0",
      "rollup-plugin-typescript2": "^0.32.0",
      "ts-jest": "29.1.0",
      "typescript": "5.1.6"
    }
  }
}
