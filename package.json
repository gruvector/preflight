{
  "name": "@upleveled/preflight",
  "version": "0.1.0",
  "author": "UpLeveled (https://github.com/upleveled)",
  "contributors": [
    "José Fernando Höwer Barbosa <josehowerupn@gmail.com>",
    "Karl Horky <karl.horky@gmail.com>"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/preflight.esm.js",
  "size-limit": [
    {
      "path": "dist/preflight.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/preflight.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.9.1",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "@upleveled/eslint-config-upleveled": "1.4.1",
    "babel-eslint": "10.0.0",
    "eslint": "7.5.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-testing-library": "3.9.0",
    "eslint-plugin-unicorn": "25.0.1",
    "husky": "4.3.7",
    "size-limit": "4.9.1",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "resolutions": {
    "eslint": "7.5.0"
  }
}
