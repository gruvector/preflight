    "lint": "tsdx lint src __tests__ --max-warnings 0",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@next/eslint-plugin-next": "10.2.3",
