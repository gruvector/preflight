{
  "name": "@upleveled/preflight",
  "version": "1.17.0",
  "author": "UpLeveled (https://github.com/upleveled)",
  "contributors": [
    "José Fernando Höwer Barbosa <josehowerupn@gmail.com>",
    "Karl Horky <karl.horky@gmail.com>"
  ],
  "repository": "upleveled/preflight",
  "license": "MIT",
  "type": "module",
  "main": "dist/preflight.esm.js",
  "module": "dist/preflight.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "preflight": "bin/preflight.js"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch --format esm --target node",
    "build": "tsdx build --format esm --target node",
    "test": "tsdx test",
    "test-local": "rm -rf ./__tests__/fixtures/__temp && yarn test",
    "lint": "tsdx lint src __tests__ --max-warnings 0",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build-docker-ts": "tsc --project docker/tsconfig.json"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.15.4",
    "@next/eslint-plugin-next": "11.1.2",
    "@size-limit/preset-small-lib": "5.0.3",
    "@types/node": "16.9.2",
    "@types/p-map": "2.0.0",
    "@types/semver": "7.3.8",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "@upleveled/eslint-config-upleveled": "1.17.1",
    "@upleveled/eslint-plugin-upleveled": "1.1.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-flowtype": "6.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.2",
    "eslint-plugin-unicorn": "36.0.0",
    "p-map": "5.1.0",
    "size-limit": "5.0.3",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "algoliasearch": "4.10.5",
    "chalk": "4.1.2",
    "cheerio": "1.0.0-rc.10",
    "depcheck": "1.4.2",
    "execa": "5.1.1",
    "listr2": "3.12.1",
    "node-fetch": "3.0.0",
    "p-reduce": "3.0.0",
    "prettier": "2.4.1",
    "readdirp": "3.6.0",
    "semver": "7.3.5",
    "top-user-agents": "1.0.38"
  },
  "resolutions": {
    "tsdx/**/node-notifier": "10.0.0",
    "eslint": "7.32.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!aggregate-error|clean-stack|escape-string-regexp|indent-string|p-map).+\\.(js|jsx)$"
    ]
  }
}
